# Aether Backend Environment Configuration

# Server Configuration
PORT=8080
HOST=0.0.0.0
GIN_MODE=debug

# Database Configuration
NEO4J_URI=bolt://localhost:7687
NEO4J_USERNAME=neo4j
NEO4J_PASSWORD=password
NEO4J_DATABASE=aether

# Redis Configuration
REDIS_ADDR=localhost:6379
REDIS_PASSWORD=
REDIS_DB=0

# Keycloak Configuration
KEYCLOAK_URL=http://localhost:8081
KEYCLOAK_REALM=aether
KEYCLOAK_CLIENT_ID=aether-backend
KEYCLOAK_CLIENT_SECRET=your-client-secret

# AWS S3 Configuration
AWS_REGION=us-east-1
AWS_ACCESS_KEY_ID=your-access-key
AWS_SECRET_ACCESS_KEY=your-secret-key
S3_BUCKET=aether-storage
S3_ENDPOINT=http://localhost:9000

# Kafka Configuration
KAFKA_BROKERS=localhost:9092
KAFKA_TOPIC_PREFIX=aether

# AudiModal API Configuration
AUDIMODAL_API_URL=https://api.audimodal.com
AUDIMODAL_API_KEY=your-api-key

# DeepLake Configuration
DEEPLAKE_URL=your-deeplake-url
DEEPLAKE_TOKEN=your-deeplake-token

# Monitoring Configuration
PROMETHEUS_ENABLED=true
OTEL_EXPORTER_OTLP_ENDPOINT=http://localhost:4317

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json